//IIFE
//Immediately Invoked Function Expression

(function() {
    console.log('running the anon function');
    return 'You ARE COOL!'
})();


// Methods!!!

// const Wes = {
//     name: 'sean ruhland',
//     sayHi: function() {
//         console.log('Hioo')
//         return 'Hey Wes'
//     },
